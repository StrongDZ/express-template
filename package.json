{
    "name": "express-template",
    "version": "1.0.0",
    "description": "",
    "main": "server.js",
    "scripts": {
        "test": "ts-node src/test.ts",
        "start": "ts-node src/server.ts",
        "dev": "nodemon src/server.ts",
        "clean-all": "rm -rf build && rm -rf node_modules",
        "clean-build": "test -d build && find build -mindepth 1 -path build/secrets -prune -o -exec rm -rf {} + || true",
        "build": "npm run clean-build && npm ci && tsc",
        "deploy": "node build/src/server.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "compression": "^1.8.1",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "ethers": "^6.15.0",
        "express": "^5.1.0",
        "express-rate-limit": "^8.2.1",
        "fs": "^0.0.1-security",
        "inquirer": "^12.10.0",
        "mongoose": "^8.19.2",
        "morgan": "^1.10.1",
        "node-cron": "^4.2.1",
        "swagger-ui-express": "^5.0.1",
        "winston": "^3.18.3"
    },
    "devDependencies": {
        "@types/compression": "^1.8.1",
        "@types/cors": "^2.8.19",
        "@types/express-rate-limit": "^5.1.3",
        "@types/morgan": "^1.9.10",
        "@types/node": "^24.10.0",
        "@types/swagger-ui-express": "^4.1.8",
        "nodemon": "^3.1.10",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3"
    }
}
